{
  "name": "<%= name %>",
  "version": "0.0.0",
  "private": true,
  "description": "<%= description %>",
  "keywords": [],
  "author": "<%= author %>",
  "license": "",
  "main": "dist/index.js",
  "dependencies": {
    "express": "^4.12.3",
    "lodash": "^4.17.4",
    "mongoose": "^4.10.1",
    "fast-json-patch": "^2.0.5",
    "swagger-express-mw": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.6",
    "should": "^7.1.0",
    "supertest": "^1.0.0",
    "mocha": "^3.4.2",
    "chai": "^3.2.0",
    "chai-as-promised": "^6.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "babel-watch src",
    "swagger": "export swagger_swagger_fileName='src/api/swagger/swagger.yaml' && swagger project edit",
    "swagger-win": "set swagger_swagger_fileName='src/api/swagger/swagger.yaml' && swagger project edit",
    "build": "babel src -d dist --copy-files",
    "watch": "babel -w src/index.js -d dist -s",
    "babel-version": "babel --version",
    "lint": "eslint src/api/ src/config/ src/models/ src/app.js src/index.js",
    "test": "mocha src/test/**/*.js --compilers js:babel-core/register -R spec --timeout 5000"
  }<%
   if (git != '') {%>,
  "repository": {
    "type": "git",
    "url": "git+<%= git %>"
  } <% } %>
}
